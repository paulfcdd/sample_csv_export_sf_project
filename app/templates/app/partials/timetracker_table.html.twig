<div class="row">
    <div class="col-6">
        <form method="get">
            <div class="span5 col-md-5" id="sandbox-container">
                <div class="input-daterange input-group" id="datepicker">
                    <input type="text" class="input-sm form-control" name="start">
                    <span class="input-group-addon">to</span>
                    <input type="text" class="input-sm form-control" name="end">
                </div>
            </div>
            <button>filter values</button>
        </form>
    </div>
    <div class="col-6">ghgf</div>
    <div class="col-12">
        <table class="table">
            <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Time spent</th>
                <th scope="col">Description</th>
                <th scope="col">Date</th>
            </tr>
            </thead>
            <tbody>
            {% for timetrackerEntry in timetrackerEntries %}
                <tr>
                    <th scope="row">{{ timetrackerEntry.id }}</th>
                    <td>{{ timetrackerEntry.timeSpent }}</td>
                    <td>{{ timetrackerEntry.description }}</td>
                    <td>{{ timetrackerEntry.createdAt | date('d.m.Y') }}</td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    </div>
</div>